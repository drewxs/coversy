{
    "name": "coversy",
    "version": "1.0.0",
    "description": "Instructor shift scheduling, shift exchange, and payroll management system. For internal use only.",
    "main": "index.js",
    "scripts": {
        "doc": "jsdoc -c ./jsdoc.json",
        "install:server": "cd ./server && npm install",
        "install:client": "cd ./client && npm install",
        "i-all": "run-p install:*",
        "start:server": "npm --prefix ./server run dev",
        "start:client": "npm --prefix ./client start",
        "s-all": "run-p start:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kito0/capstone.git"
    },
    "keywords": [],
