    "license": "ISC",
    "bugs": {
        "url": "https://github.com/kito0/capstone/issues"
    },
    "homepage": "https://github.com/kito0/capstone#readme",
    "devDependencies": {
        "clean-jsdoc-theme": "^3.3.4",
        "jsdoc": "^3.6.10",
        "npm-run-all": "^4.1.5"
    }
}
